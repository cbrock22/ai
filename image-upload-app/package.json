{
  "name": "image-upload-app",
  "version": "1.0.0",
  "description": "Secure image upload application with user authentication and folder-based permissions",
  "scripts": {
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:clean": "docker-compose -f docker-compose.dev.yml down -v",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build -d",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "docker:prod:clean": "docker-compose -f docker-compose.prod.yml down -v",
    "docker:prod:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "docker:prod:restart": "docker-compose -f docker-compose.prod.yml restart",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:backend": "docker-compose logs -f backend",
    "docker:logs:mongodb": "docker-compose logs -f mongodb",
    "docker:ps": "docker-compose ps",
    "docker:restart": "docker-compose restart",
    "docker:check": "bash scripts/check-docker.sh || scripts\\check-docker.bat",
    "docker:clean": "bash scripts/docker-clean.sh",
    "db:backup": "bash scripts/db-backup.sh",
    "db:backup:prod": "bash scripts/backup-prod.sh",
    "db:restore": "bash scripts/db-restore.sh",
    "db:restore:prod": "bash scripts/restore-prod.sh",
    "deploy": "bash scripts/deploy.sh",
    "logs:prod": "bash scripts/logs-prod.sh",
    "start:dev": "bash scripts/start-dev.sh || scripts\\start-dev.bat",
    "start:prod": "bash scripts/start-prod.sh || scripts\\start-prod.bat",
    "install:all": "cd backend && npm install && cd ../frontend && npm install",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/image-upload-app"
  },
  "keywords": [
    "image-upload",
    "authentication",
    "mongodb",
    "docker",
    "react",
    "express",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "openssl": "^2.0.0"
  }
}
